{
    "name": "fizz/wizard-plugin",
    "type": "composer-plugin",
    "description": "Automatically registers bundles in a Symfony2 application when running 'composer require'",
    "keywords": ["wizard", "install", "bundle", "symfony"],
    "homepage": "https://github.com/dj-dirk/wizard-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Lo√Øc Chardonnet",
        "email": "loic.chardonnet@gmail.com",
        "homepage": "http://gnugat.github.io",
        "role": "Developer"
    },{
        "name":"Dirk Jansen",
        "email": "dirk@fizz.nl"
    }],
    "require": {
        "composer-plugin-api": "~1",
        "fizz/wizard-bundle": "~1.0"
    },
    "require-dev": {
        "composer/composer": "~1.0@alpha"
    },
    "autoload": {"psr-4": {
        "Gnugat\\ComposerPlugin\\WizardPlugin\\": "src/Gnugat/ComposerPlugin/WizardPlugin"
    }},
    "extra": {
        "class": "Gnugat\\ComposerPlugin\\WizardPlugin\\GnugatWizardPlugin"
    }
}
