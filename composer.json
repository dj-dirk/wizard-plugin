{
    "name": "gnugat/wizard-plugin",
    "type": "composer-plugin",
    "description": "Automatically registers bundles in a Symfony2 application when running 'composer require'",
    "keywords": ["wizard", "install", "bundle", "symfony"],
    "homepage": "https://github.com/gnugat/wizard-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Lo√Øc Chardonnet",
        "email": "loic.chardonnet@gmail.com",
        "homepage": "http://gnugat.github.io",
        "role": "Developer"
    }],
    "require": {
        "composer-plugin-api": "1.0.0",
        "gnugat/redaktilo": "~1.7"
    },
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "~1.1",
        "composer/composer": "~1.0@alpha",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": { "Gnugat\\WizardPlugin\\": "src/Gnugat/WizardPlugin" }
    },
    "autoload-dev": {
        "psr-4": { "Gnugat\\WizardPlugin\\Tests\\": "tests" }
    },
    "extra": {
        "class": "Gnugat\\WizardPlugin\\GnugatWizardPlugin"
    }
}
